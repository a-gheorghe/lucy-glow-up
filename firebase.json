{
  "hosting": {
    "public": "build",
    "site": "lucy-glow-up",
    "predeploy": "npm install && npm run build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}